<div class="navbar " >
	  <div class="col-lg-12">
	    <form class="form_phone" name="formPhone">
	    	<div class="col-lg-4 col-md-3">
	    		<!-- Username -->
	      		<input type="text" ng-model="phoneName" class="form-control"  placeholder="Phone Name identification" ng-required="true">
	      		<input type="text" ng-model="phoneAge" class="form-control" placeholder="Phone Age" ng-required="true">
	      		<input type="text" ng-model="phoneInfo" class="form-control" placeholder="Phone general info" ng-required="true">
	      		<input type="text" ng-model="phoneImgurl" class="form-control" placeholder="Phone image url"  >
          	<button class="btn btn-default col-lg-2 col-md-2" ng-click="formPhone.$valid && addPhone()">Add</button>
          	</div>
          	{{message}}
	    </form>
	    
	  </div>
	</div>